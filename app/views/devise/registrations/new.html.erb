<h2>Sign up</h2>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :wrapper => :inline) do |f| %>
  <!-- <%= devise_error_messages! %> -->
  <fieldset>
    <legend>Fields</legend>
    
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
  </fieldset>
  <div><%= f.submit "Sign up" %></div>
  
<% end %>

<%= render "links" %>
